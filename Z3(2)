#include <iostream>
#include <math.h>
#include <stdio.h> 
using namespace std;
int main (int arg, char** argv)
{
   int a,b,z,x,e,sin,cos,ln;
   int Y;
   switch (a,b,z)
{  case 1:
    a=0.8;
    b=1.1;
    z=ln*(b*x^3);
    if (x<a)
    cout<<"(2,8*sin^2*(a*x)-b*x^3*z="<<Y;
}  switch (a,b,z)
{ case 2:
   a=2.2;
   b=1.3;
   z=ln*(b*x);
   if ((a<=x)&&(x<=b^2))
   cout<<"z*cos*(a*x+b^2+ln*z)="<<Y;        
}  switch(a,b,z)
{  case 3:
   a=3.2;
   b=1.3;
   e=2.71;
   z=ln*(b*x^2);
   if(x>b^2)
   cout<<"(e^(2,5*a*x)+z*a*b*x)="<<Y;
    } 
return Y;
}
